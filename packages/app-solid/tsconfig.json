{
  "extends": "@tsconfig/strictest",
  "include": ["src"],
  "compilerOptions": {
    "exactOptionalPropertyTypes": false,
    "noUncheckedIndexedAccess": false,
    "checkJs": false,
    "noEmit": true,
    "module": "esnext",
    "moduleResolution": "node",
    "target": "esnext",
    "lib": ["esnext", "dom"],
    "types": ["vite/client"],
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}
