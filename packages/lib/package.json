{
  "name": "@hiogawa/unocss-preset-antd",
  "version": "2.1.0-pre.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./dist/reset.css": {
      "import": "./dist/reset.css",
      "require": "./dist/reset.css"
    }
  },
  "files": [
    "dist/index.*",
    "dist/reset.*"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hi-ogawa/unocss-preset-antd",
    "directory": "packages/lib"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "run-s build:*",
    "build:reset": "vite build && cp -f dist/vite/assets/index-*.css dist/reset.css",
    "build:tsup": "tsup",
    "build-theme": "pnpm -s ts ./src/build-theme.ts > ./src/theme.ts",
    "generate-tw": "unocss-typescript-dsl --outDir src/tw",
    "ts": "node -r esbuild-register",
    "release": "pnpm publish --no-git-checks --access public"
  },
  "devDependencies": {
    "@types/node": "^16.18.12",
    "@unocss/preset-uno": "^0.48.5",
    "antd": "^5.1.7",
    "esbuild": "^0.17.5",
    "esbuild-register": "^3.4.2"
  },
  "peerDependencies": {
    "@unocss/preset-uno": "*",
    "unocss": "*"
  },
  "dependencies": {
    "@unocss/reset": "^0.48.5",
    "local-pkg": "^0.4.3"
  }
}
