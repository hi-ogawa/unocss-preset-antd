{
  "name": "@hiogawa/unocss-preset-antd",
  "version": "1.4.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hi-ogawa/unocss-preset-antd",
    "directory": "packages/lib"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "build-theme": "pnpm -s ts ./src/build-theme.ts > ./src/theme.ts",
    "tw-api": "unocss-typescript-dsl --out-file src/tw-api.ts --filter-colors 'xxx'",
    "ts": "node -r esbuild-register",
    "release": "pnpm publish --no-git-checks --access public"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@unocss/preset-uno": "^0.48.5",
    "antd": "^5.1.7",
    "esbuild": "^0.17.5",
    "esbuild-register": "^3.4.2",
    "lodash": "^4.17.21",
    "unocss": "^0.48.5"
  },
  "peerDependencies": {
    "@unocss/preset-uno": "*",
    "unocss": "*"
  }
}
