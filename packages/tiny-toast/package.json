{
  "name": "@hiogawa/tiny-toast",
  "version": "0.1.1-pre.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./dist/react": {
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.cjs",
      "types": "./dist/react/index.d.ts"
    },
    "./dist/preact": {
      "import": "./dist/preact/index.js",
      "require": "./dist/preact/index.cjs",
      "types": "./dist/preact/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "homepage": "https://github.com/hi-ogawa/unocss-preset-antd/tree/main/packages/tiny-toast",
  "repository": {
    "type": "git",
    "url": "https://github.com/hi-ogawa/unocss-preset-antd/",
    "directory": "packages/tiny-transition"
  },
  "scripts": {
    "dev": "vite --no-clearScreen",
    "build": "pnpm run --seq /^build:/",
    "build:tsup": "tsup",
    "xbuild:unocss": "unocss dist/react/index.js dist/react/index.cjs --write-transformed --out-file dist/react/index.css",
    "xbuild:inject-css": "node misc/inject-css.mjs",
    "release": "pnpm publish --no-git-checks --access public"
  },
  "devDependencies": {
    "@hiogawa/tiny-transition": "workspace:*",
    "@hiogawa/unocss-preset-antd": "workspace:*",
    "@hiogawa/utils": "1.6.1-pre.7",
    "@hiogawa/utils-react": "^1.3.1-pre.0",
    "@types/react": "^18.2.14",
    "preact": "^10.15.1",
    "react": "^18.2.0"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}
